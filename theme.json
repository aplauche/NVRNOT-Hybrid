{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": false,
			"palette": [
				{
					"color": "#0000FF",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6d28d9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#fff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "oklch(98% 0 0)",
					"name": "Base 100",
					"slug": "base-100"
				},
				{
					"color": "oklch(95% 0 0)",
					"name": "Base 200",
					"slug": "base-200"
				},
				{
					"color": "oklch(92% 0 0)",
					"name": "Base 300",
					"slug": "base-300"
				},
				{
					"color": "#141414",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#707070",
					"name": "Secondary Contrast",
					"slug": "contrast-alt"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"xs": 1.1,
				"sm": 1.25,
				"md": 1.5,
				"lg": 1.75,
				"xl": 2
			},
			"borderRadius": {
				"sm": "4px",
				"md": "8px",
				"lg": "16px",
				"xl": "32px",
				"full": "999999999px"
			}
		},
		"layout": {
			"contentSize": "960px",
			"wideSize": "1280px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "8",
					"size": "8px",
					"slug": "8"
				},
				{
					"name": "16",
					"size": "16px",
					"slug": "16"
				},
				{
					"name": "24",
					"size": "24px",
					"slug": "24"
				},
				{
					"name": "32",
					"size": "32px",
					"slug": "32"
				},
				{
					"name": "48",
					"size": "48px",
					"slug": "48"
				},
				{
					"name": "64",
					"size": "64px",
					"slug": "64"
				},
				{
					"name": "Section",
					"size": "clamp(50px, 10vw + 1rem, 120px)",
					"slug": "section"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": {
				"maxViewportWidth": "1280px",
				"minViewportWidth": "640px"
			},
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Outfit",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/outfit/Outfit-Variable.woff2"
							]
						}
					],
					"fontFamily": "Outfit, sans-serif",
					"name": "Outfit",
					"slug": "outfit"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Extra Small",
					"size": "0.75rem",
					"slug": "xs"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.2rem",
					"slug": "md"
				},
				{
					"fluid": {
						"max": "1.6rem",
						"min": "1.2rem"
					},
					"name": "Large",
					"size": "1.6rem",
					"slug": "lg"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "xLarge",
					"size": "2rem",
					"slug": "xl"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "2rem"
					},
					"name": "xxLarge",
					"size": "2.5rem",
					"slug": "xxl"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "xxxLarge",
					"size": "3rem",
					"slug": "xxxl"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Elevation 100",
					"shadow": "0 1px 1px 0 rgba(48, 49, 51, 0.1), 0 0 1px 0 rgba(48, 49, 51, 0.05)",
					"slug": "elevation-100"
				},
				{
					"name": "Elevation 200",
					"shadow": "0 2px 4px 0 rgba(48, 49, 51, 0.1), 0 0 1px 0 rgba(48, 49, 51, 0.05)",
					"slug": "elevation-200"
				},
				{
					"name": "Elevation 300",
					"shadow": "0 4px 8px 0 rgba(48, 49, 51, 0.1), 0 0 1px 0 rgba(48, 49, 51, 0.05)",
					"slug": "elevation-300"
				},
				{
					"name": "Elevation 400",
					"shadow": "0 8px 1rem 0 rgba(48, 49, 51, 0.1), 0 0 1px 0 rgba(48, 49, 51, 0.05)",
					"slug": "elevation-400"
				}
			]
		},
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "xSmall",
							"size": "4px",
							"slug": "4"
						},
						{
							"name": "Small",
							"size": "8px",
							"slug": "8"
						},
						{
							"name": "Medium",
							"size": "12px",
							"slug": "12"
						},
						{
							"name": "Large",
							"size": "16px",
							"slug": "16"
						},
						{
							"name": "xLarge",
							"size": "24px",
							"slug": "24"
						},
						{
							"name": "xLarge",
							"size": "32px",
							"slug": "32"
						}
					]
				}
			}
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "0.75rem",
								"right": "1.5rem",
								"bottom": "0.75rem",
								"left": "1.5rem"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/calendar": {
				"color": {
					"background": "var(--wp--preset--color--white)",
					"text": "var(--wp--preset--color--black)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"fontFamily": "var(--wp--preset--font-family--body)"
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--32)",
						"right": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)",
						"left": "var(--wp--preset--spacing--32)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--md)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--md)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--md)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--32)",
						"right": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)",
						"left": "var(--wp--preset--spacing--32)"
					},
					"margin": {
						"bottom": "0",
						"top": "0"
					}
				},
				"elements": {
					"heading": {
						"typography": {
							"fontWeight": "600"
						}
					}
				}
			},
			"core/file": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"style": "solid",
					"width": "0 0 2px 0"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "0",
						"bottom": "1rem",
						"left": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--body)",
					"fontSize": "var(--wp--preset--font-size--sm)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--body)",
							"fontSize": "var(--wp--preset--font-size--sm)"
						}
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--32)"
					}
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--24)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/post-comments-form": {
				"css": " & .form-submit {margin-bottom: 0;}"
			},
			"core/post-template": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--32);"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--32)",
						"right": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)",
						"left": "var(--wp--preset--spacing--32)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				},
				"css": "& p {margin: 0;}",
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--sm)",
							"fontStyle": "inherit"
						}
					}
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--32)",
						"right": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)",
						"left": "var(--wp--preset--spacing--32)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)",
					"lineHeight": "var(--wp--custom--line-height--md)"
				}
			},
			"core/query-pagination": {
				"css": "margin-top: var(--wp--preset--spacing--32) !important;"
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				},
				"css": "& p {margin: 0;}",
				"elements": {
					"cite": {
						"spacing": {
							"margin": {
								"top": "10px"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--sm)",
							"fontStyle": "inherit"
						}
					}
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--32)",
						"right": "var(--wp--preset--spacing--32)",
						"bottom": "var(--wp--preset--spacing--32)",
						"left": "var(--wp--preset--spacing--32)"
					}
				}
			},
			"core/search": {
				"elements": {
					"button": {
						"border": {
							"width": "6px"
						},
						"spacing": {
							"padding": {
								"top": "0.5rem",
								"right": "1rem",
								"bottom": "0.5rem",
								"left": "1rem"
							}
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--xs)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;color: currentColor}",
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base-100)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--md)",
					"style": "solid",
					"width": "1px",
					"color": "var(--wp--preset--color--contrast)"
				},
				"color": {
					"background": "var(--wp--preset--color--white)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"css": "{width: auto;}",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--md)"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.25rem",
						"bottom": "0.75rem",
						"left": "1.25rem"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"outline": {
						"color": "var(--wp--preset--color--primary)",
						"width": "4px"
					}
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--xs)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "0px",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--24)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--24)"
			}
		},
		"typography": {
			"fontFamily": "monospace",
			"fontSize": "var(--wp--preset--font-size--sm)",
			"fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--lg)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}